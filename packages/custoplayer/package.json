{
  "name": "custoplayer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "yarn jest src",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "lint": "eslint './src/**/*.{tsx,ts}'",
    "lint-fix": "eslint './src/**/*.{tsx,ts}' --fix",
    "test:coverage": "yarn test -- --coverage",
    "test:badges": "yarn test:coverage  && jest-coverage-badges --output './badges'"
  },
  "dependencies": {
    "framer-motion": "^8.5.0",
    "jotai": "^1.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "5.3.5",
    "ts-jest": "^29.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.0.1",
    "acorn-jsx": "5.3.2",
    "babel-jest": "^29.4.3",
    "babel-loader": "8.2.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "jest": "^29.4.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.4.3",
    "json": "11.0.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "storybook-builder-vite": "0.1.21",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "framer-motion": "8.5.0",
    "jotai": "1.13.1",
    "react": "17.x || 18.x",
    "react-dom": " 17.x || 18.x",
    "styled-components": "5.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/custoplayer.umd.js",
  "module": "./dist/custoplayer.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/custoplayer.es.js",
      "require": "./dist/custoplayer.umd.js"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/lib/components/**/*.tsx"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1"
    },
    "testEnvironment": "jsdom"
  }
}
